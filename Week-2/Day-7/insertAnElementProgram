public class insertAnElementProgram {

	public static void main(String[] args) {
		int a[]= new int[7]{5,8,3,1,7,9,0};
		//len=7-2
		int index=3;
		int x=10;
		for(int i=a.length-2;i>=index;i--)
		{
			a[i+1]=a[i];
		}
		a[index]=x;
		for(int i=0;i<a.length;i++)
		{
			System.out.print(a[i]+" ");
		}
	}
}
