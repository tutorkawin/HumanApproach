class DecendingOrderSortingProgram {
    public static void main(String[] args) {
        int arr[]={7,9,0,2,8,5};
        for(int i=0;i<arr.length;i++)
        {
        	for(int j=i+1;j<arr.length;j++)
        	{
        		if(arr[i]<arr[j])
        		{
        			int c=arr[i];
        			arr[i]=arr[j];
        			arr[j]=c;
        		}
        	}
        }
        for( int i=0; i<arr.length ;i++)
        {
            System.out.print(arr[i]+" ");
        }
    }
}

o/p:
9 8 7 5 2 0 
